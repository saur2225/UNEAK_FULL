        <form method="post">
        <div class="row">
        	<div class="col-md-6">
            	<div class="heading_s1">
            		<h4 style="color:white">Billing Details</h4>
                </div>
                    <div class="form-group">
                        <input type="text" required="" disabled value="{{order.address.name}}" class="form-control" name="name" placeholder="Name *">
                    </div>
                    <div class="form-group">
                        <input class="form-control" disabled required="" value="{{order.address.landmark}}" type="text" name="landmark" placeholder="Company Name/Landmark">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" disabled value="{{order.address.address_line1}}" name="address_line1" required="" placeholder="Address *">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" disabled value="{{order.address.address_line2}}" name="address_line2" required="" placeholder="Address line 2">
                    </div>
                    <div class="form-group">
                        <input class="form-control" required="" disabled type="text" value="{{order.address.city}}" name="city" placeholder="City *">
                    </div>
                    <div class="form-group">
                        <input class="form-control" required="" disabled type="text" name="state" value="{{order.address.state}}" placeholder="State *">
                    </div>
                    <div class="form-group">
                        <input class="form-control" required=""  disabled type="text" name="country" value="{{order.address.country}}" placeholder="country *">
                    </div>
                    <div class="form-group">
                        <input class="form-control" required="" disabled type="text" name="pincode" value="{{order.address.pincode}}" placeholder="Pincode *">
                    </div>
                    <div class="form-group">
                        <input class="form-control" required="" disabled type="text" name="contact" value="{{order.address.contact}}" placeholder="Phone*">
                    </div>
                    <div class="form-group">
                        <input class="form-control" required="" disabled type="text" value="{{order.address.email}}" name="email" placeholder="Email address *">
                    </div>
