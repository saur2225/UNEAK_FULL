            </div>
            <div class="col-md-6">
                <div class="order_review">
                    <div class="heading_s1">
                        <h4>{{#if user.isAdmin}}Order Details {{else}}Your Orders{{/if}}</h4>
                    </div>
                    <div class="table-responsive order_table">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Product</th>
                                    <th>Total</th>
                                </tr>
                            </thead>
                            <tbody>
                              {{#each products}}
                                <tr>
                                    <td><a style="color: #212529;" href="/product/detail/{{this.item.slug}}">{{this.item.name}}</a><br> Size Choosed <span style="text-transform: uppercase;">{{this.item.size_selected}}</span> <span class="product-qty">x {{qty}}</span></td>
                                    <td>₹ {{this.price}}</td>
                                </tr>
                                <tr class="product_color_switch">
                                  <td>Color Choosed <span class="active" data-color="{{this.item.color_selected}}"></span></td>
                                </tr>
                                {{/each}}
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Referal Discount</th>
                                    <td class="product-subtotal">-₹ {{order.cart.referal_bonus}}</td>
                                </tr>
                                <tr>
                                    <th>SubTotal</th>
                                    <td class="product-subtotal">₹ {{order.cart.totalPrice}}</td>
                                </tr>
                                <tr>
                                    <th>Shipping</th>
                                    <td>Free Shipping</td>
                                </tr>
                                <tr>
                                    <th>Total</th>
                                    <td class="product-subtotal">₹ {{order.cart.totalPrice}}</td>
                                </tr>
                                {{#if user.isAdmin}}
                                <tr>
                                    <th><h5>Ordered By:</h5></th>
                                    <td class="product-subtotal"></td>
                                </tr>
                                <tr>
                                    <th>Name:</th>
                                    <td class="product-subtotal">{{userDetail.name}}</td>
                                </tr>
                                <tr>
                                    <th>Contact no.</th>
                                    <td class="product-subtotal">{{userDetail.contact}}</td>
                                </tr>
                                <tr>
                                    <th>E mail</th>
                                    <td class="product-subtotal">{{userDetail.email}}</td>
                                </tr>
                                {{/if}}
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
            </form>
