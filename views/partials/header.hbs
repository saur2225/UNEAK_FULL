      <div class="middle-header dark_skin" style="background-color:black;position:relative;">
        <div class="container">
          <div class="nav_block">
          <img src="/assets\images\logo\two_w.png" style="width:70px; margin-left:-120px">
            <a class="navbar-brand" href="/">
              <img
                class="logo_light"
                src="assets\images\four_ww.png"
                alt="logo"
              />
              <img
                class="logo_dark" id="sizze" 
                src="/assets\images\logo\four_ww.png" 
                alt="logo"
              />
            </a>
            
            <ul class="navbar-nav attr-nav align-items-center">
              <li>
                <a href="/user/me" class="nav-link"
                  ><i class="linearicons-user" style="color:white;cursor: pointer"></i
                ></a>
              </li>
              <li class="dropdown cart_dropdown">
                <a class="nav-link cart_trigger" href="#" data-toggle="dropdown"
                  ><i {{#if cart}} {{else}}onclick="window.open('/shop-cart','_self')" {{/if}} class="linearicons-bag2" style="color:white;cursor: pointer;"></i
                  >{{#if session.cart}}<span class="cart_count">{{session.cart.totalQty}}</span
                  >{{/if}}<span class="amount"
                    ><span class="currency_symbol">₹ </span>{{#if session.cart}}{{session.cart.totalPrice}}{{else}}0{{/if}}</span
                  ></a
                >
                {{#if cart}}
                <div
                  class="cart_box cart_right dropdown-menu dropdown-menu-right"
                >
                  <ul class="cart_list">
                    {{#each cart}}
                    <li>
                      <a href="#" class="item_remove"
                        ><i class="ion-close"></i
                      ></a>
                      <a href="#"
                        ><img
                          src="data:image/jpeg;base64,{{this.item.image}}"
                          alt="cart_thumb1" width="80px" height="80px"
                        /> <span class="price">{{this.item.name}}</span></a
                      >
                      <span class="cart_quantity">
                        {{this.qty}} x
                        <span class="cart_amount">
                          <span class="price_symbole">₹ </span></span
                        >{{this.item.price}}</span
                      >
                    </li>
                    {{/each}}
                  </ul>
                  <div class="cart_footer">
                    <p class="cart_total">
                      <strong>Subtotal:</strong>
                      <span class="cart_price">
                        <span class="price_symbole">₹</span></span
                      >{{session.cart.totalPrice}}
                    </p>
                    <p class="cart_buttons">
                      <a href="/shop-cart" class="btn btn-fill-line view-cart"
                        >View Cart</a
                      ><a href="/checkout" class="btn btn-fill-out checkout"
                        >Checkout</a
                      >
                    </p>
                  </div>
                </div>
                {{/if}}
              </li>
            </ul>
